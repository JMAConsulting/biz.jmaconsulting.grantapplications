    <url desc="Support">https://github.com/JMAConsulting/biz.jmaconsulting.grantapplications/issues?state=open</url>
    <url desc="Main Extension Page">
      https://github.com/JMAConsulting/biz.jmaconsulting.grantapplications/wiki/About-grant-applications
    </url>
    <url desc="Installation Instructions for 1.5">https://github.com/JMAConsulting/biz.jmaconsulting.grantapplications/blob/4.7/README.md</url>
  </urls>
