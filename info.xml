    <author>JMA Consulting</author>
    <email>joe.murray@jmaconsulting.biz</email>
  </maintainer>
  <releaseDate>2017-09-04</releaseDate>
  <version>1.5.2</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Grantapplications</namespace>
  </civix>
</extension>
